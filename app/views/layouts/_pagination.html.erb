<span class="flex items-center justify-center text-base text-gray-700 dark:text-gray-400">
  Showing <span class="font-semibold text-gray-900">&nbsp;<%= pagination.page %>&nbsp;</span> of <span class="font-semibold text-gray-900">&nbsp;<%= pagination.total_pages %></span>&nbsp; Entries
</span>
<br>
<div class="flex items-center justify-center">
  <% if  pagination.previous_page? %>
    <%= link_to 'First', users_path(page: 1), class: "inline-flex items-center px-4 py-2 text-base font-medium text-white bg-gray-800 rounded-l hover:bg-gray-900 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white" %>
    <%= link_to users_path(page: pagination.previous_page), class: "inline-flex items-center px-4 py-2 text-base font-medium text-white bg-gray-800 hover:bg-gray-900 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white" do %>
      <svg aria-hidden="true" class="w-5 h-5 mr-2" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M7.707 14.707a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414l4-4a1 1 0 011.414 1.414L5.414 9H17a1 1 0 110 2H5.414l2.293 2.293a1 1 0 010 1.414z" clip-rule="evenodd"></path></svg>
      Previous
    <% end %>
  <% else %>
    <p class="inline-flex items-center px-4 py-2 text-base font-medium text-white bg-gray-800 rounded-l hover:bg-gray-900 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white">First</p>
    <p class="inline-flex items-center px-4 py-2 text-base font-medium text-white bg-gray-800 hover:bg-gray-900 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white">Previous</p>
  <% end %>
  <% if pagination.next_page? %>
    <%= link_to users_path(page: pagination.next_page), class: "inline-flex items-center px-4 py-2 text-base font-medium text-white bg-gray-800 border-0 border-l border-gray-700 hover:bg-gray-900 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white"  do %>
      Next
      <svg aria-hidden="true" class="w-5 h-5 ml-2" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M12.293 5.293a1 1 0 011.414 0l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-2.293-2.293a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg>
    <% end %>
    <%= link_to 'Last', users_path(page: pagination.total_pages), class: "inline-flex items-center px-4 py-2 text-base font-medium text-white bg-gray-800 rounded-r hover:bg-gray-900 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white" %>
  <% else %>
    <p class="inline-flex items-center px-4 py-2 text-base font-medium text-white bg-gray-800 border-0 border-l border-gray-700 hover:bg-gray-900 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white">Next</p>
    <p class="inline-flex items-center px-4 py-2 text-base font-medium text-white bg-gray-800 rounded-r hover:bg-gray-900 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white">Last</p>
  <% end %>
</div>
